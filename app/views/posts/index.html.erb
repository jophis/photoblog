<p id="notice"><%= notice %></p>

<h1> Photo Blog </h1>


    <% @posts.each do |post| %>
        <h2><%= link_to post.title, post %></h2>
        <a href= "<%= post_url(post) %>"><%= image_tag(post.image.tiny.url) %></a>
        <%= post.description %>
        <br>
        <hr>
    <% end %>

<br>


